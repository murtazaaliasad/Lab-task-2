#include <stdio.h>

int main()
{
    int seconds, i, j;

    printf("Enter number of seconds: ");
    scanf("%d", &seconds);

    for (i = seconds; i > 0; i--)
    {
        printf("%d\n", i);

        for (j = 0; j < 1000000000; j++)
        {
        }
    }

    printf("Time's up!\n");

    return 0;
}
