#include <stdio.h>

int main()
{
    int num, i = 1;

    printf("Enter a number: ");
    scanf("%d", &num);

    if (num >= 1)
    {
        do
        {
            printf("%d\n", i);
            i++;
        }
        while (i <= num);
    }

    printf("Countdown complete!\n");

    return 0;
}
